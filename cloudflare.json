{
	"name": "MobDirect",
	"description": "Browser Direction",
	"category": "webmaster",
	"version": "0.1",
	"main": "./public/javascripts/abetterbrowser.min.js",
	"implements": "AMD",
	"contributors":
	[
		{
			"name": "Mike D.",
			"email": "mdawson40@gmail.com"
		},
