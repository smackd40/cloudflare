{
	"name": "A Better Browser",
	"description": "Warns users with a simple message at the top of your website if their browser version is too old.",
	"category": "webmaster",
	"version": "1.2.3",
	"main": "./public/javascripts/abetterbrowser.min.js",
	"implements": "AMD",
	"contributors":
	[
		{
			"name": "Mike D.",
			"email": "mdawson40@gmail.com"
		},
		{
			"name": "Kevin F.",
			"email": "kfinn@gmail.com"
		}
	],
	"repository":
	{
		"type": "git",
		"url": "https://github.com/smackd40/cloudflare.git"
	},
	"config":
	{
		"assets":
		{
			"logos":
			{
				"200px": "./public/images/rocket-200.png",
				"132px": "./public/images/rocket-132.png"
			},
			"detail_page":
			[
				"./public/images/example.png"
			]
		},
		"details":
		{
			"Category": "Webmaster Tools",
			"Language": "English",
			"Restrictions": "None",
		},
		"interface":
		[
			{
				"id": "ie",
				"name": "Internet Explorer",
				"description": "Highest version of IE to display warning (Setting this to 7.0 will display warning in IE7 and lower, but not in IE8)",
				"type": "select",
				"options":
				[
					{
						"label": "6.0",
						"value": "6"
					},
					{
						"label": "7.0",
						"value": "7"
					},
					{
						"label": "8.0",
						"value": "8",
						"selected": true
					},
					{
						"label": "9.0",
						"value": "9"
					},
					{
						"label": "10.0",
						"value": "10"
					}
				]
			}
		]
	}
}
