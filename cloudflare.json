	{
		"type": "git",
		"url": "https://github.com/xPaw/CF-ABetterBrowser.git"
	},
	"config":
	{
		"assets":
		{
			"logos":
			{
				"200px": "./public/images/logo-200.png",
				"132px": "./public/images/logo-132.png"
			},
			"detail_page":
			[
				"./public/images/info.png"
			]
		},
		"details":
		{
			"Category": "Webmaster Tools",
			"Language": "English",
			"Restrictions": "None",
			"Supporting": "<a href='http://rocketMD.com/'>Mobile â€“ Google</a>"
		},
		"interface":
