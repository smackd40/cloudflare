					{
						"label": "7.0",
						"value": "7"
					},
					{
						"label": "8.0",
						"value": "8",
						"selected": true
					},
					{
						"label": "9.0",
						"value": "9"
					},
					{
						"label": "10.0",
						"value": "10"
					}
				]
			}
